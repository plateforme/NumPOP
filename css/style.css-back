.carousel-caption h3{
font-size:1.8em;
font-style: normal;
}


.carousel-inner>.item>img, .carousel-inner>.item>a>img {
height: auto;
width: 100%;
-webkit-animation: fadein 4s; /* Safari, Chrome and Opera > 12.1 */
-moz-animation: fadein 4s; /* Firefox < 16 */
-ms-animation: fadein 4s; /* Internet Explorer */
-o-animation: fadein 4s; /* Opera < 12.1 */
animation: fadein 4s;
}


          @keyframes fadein {
          from { opacity: 0; }
          to   { opacity: 1; }
          }

          /* Firefox < 16 */
          @-moz-keyframes fadein {
          from { opacity: 0; }
          to   { opacity: 1; }
          }
          
          /* Safari, Chrome and Opera > 12.1 */
          @-webkit-keyframes fadein {
              from { opacity: 0; }
              to   { opacity: 1; }
          }
          
          /* Internet Explorer */
          @-ms-keyframes fadein {
              from { opacity: 0; }
              to   { opacity: 1; }
          }
          
          /* Opera < 12.1 */
          @-o-keyframes fadein {
              from { opacity: 0; }
              to   { opacity: 1; }
          }
 
 
 /*  CARROUSEL OUTILS FRONTPAGE  */
section#block-views-nos-outils-block {
margin-bottom: 5%;
margin-top: 5%;
} 
 
.jcarousel-skin-default span a {
color:#fff;
background:#e31d23;
font-style: normal;
text-transform:uppercase;
padding:0 10px 0 10px;
font-size: 0.7em;
} 


.jcarousel-skin-default .jcarousel-item {
padding: 0;
width: 250px;
height: 100px;
overflow: hidden;
border: 1px solid #CCC;
list-style: none;
background: #fff none;
}

.jcarousel-skin-default .jcarousel-container-horizontal {
width: 90%;
height: auto;
padding: 20px 40px;

}

.jcarousel-skin-default .jcarousel-clip-horizontal {
width: 90%;
overflow: hidden;
}

.jcarousel-skin-default .jcarousel-prev-horizontal {
position: absolute;
top: 43%;
left: 10px;
background-position: 0 0;
}

.jcarousel-skin-default .jcarousel-next-horizontal {
position: absolute;
top: 43%;
right: 10px;
background-position: 0 -32px;
}

.jcarousel-item .views-field.views-field-title {
position: relative;
text-align: center;
font-size: x-large;
top: 50%;
  transform: translateY(-50%);
}


.jcarousel-view--nos-outils--block img {
margin-top: -34px;
}

 

 
