/*  SLIDESHOW FRONTPAGE  */

.slideshow h3 a {
color:#fff;
background:#e31d23;
font-style: normal;
text-transform:uppercase;
padding:0 10px 0 10px;
}


.field-name-field-tagline, .carousel-caption .tagline {
text-transform:uppercase; 
font-size:19px;
}    
 
  
 #views-bootstrap-carousel-2 .carousel-inner>.item>img, .carousel-inner>.item>a>img {
height: auto;
width: 1em;
-webkit-animation: fadein 4s;
-moz-animation: fadein 4s;
-ms-animation: fadein 4s;
-o-animation: fadein 4s;
animation: fadein 4s;
}
 
.carousel-caption { 
-webkit-animation: fadein 5s; /* Safari, Chrome and Opera > 12.1 */
-moz-animation: fadein 5s; /* Firefox < 16 */
-ms-animation: fadein 5s; /* Internet Explorer */
-o-animation: fadein 5s; /* Opera < 12.1 */
animation: fadein 5s;
}
            
            
.carousel-caption {
position: absolute;
right: 15%;
bottom: 20px;
z-index: 10;
padding-top: 20px;
padding-bottom: 20px;
color: #fff;
text-align: left;
text-shadow: 0 0 0;
}

.carousel-caption h3 a {
color:#fff;
}
 
.carousel-caption h3 a:visited {
color:#fff;
}
 
 
